$C = "../Common.ui";

$C.@PageOverlay {
  Group {
    LayoutMode: Center;
    Anchor: (Full: 1);

    Group #RootPanel {
      Anchor: (Width: 1000, Height: 760);
      LayoutMode: Top;
      Background: #10151c(0.96);
      Padding: (Left: 20, Top: 16, Right: 20, Bottom: 16);

      Label #Title {
        Anchor: (Height: 40);
        Text: "Ninja Tags";
        Style: (FontSize: 28, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center);
      }

      Label #EmptyLabel {
        Anchor: (Height: 26);
        Text: "You don't own any tags yet.";
        Style: (FontSize: 16, TextColor: #d1d5db, HorizontalAlignment: Center);
        Visible: false;
      }

      Group { Anchor: (Height: 10); }

